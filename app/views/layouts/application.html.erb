  </head>
  <body>
    <div id="outer-wrap">
      

    <nav class="navbar navbar-default <%= 'navbar-signed-in navbar-fixed-top' if user_signed_in? %>">
        <div class="container-fluid">
          <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
              <span class="sr-only">Toggle navigation</span><i class="fa fa-bars"></i> <%= t "pages.header.nav.menu" %> 
                </button>
                <%= link_to root_path do %>
                   <p class="navbar-brand"><span style="font-family: 'Pacifico', sans-serif; font-size: 150%;">minibarra</span> | Chef</p>
                <% end %>
            </div>

          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
              <% if user_signed_in? %>
                <% if current_user.subscribed == true %>
                  <li class="<%= 'active-nav' if params[:stage] == "Morning" || params[:stage] == "Afternoon" ||params[:stage] == "Evening" %>">
                    <%= link_to t("pages.header.nav.today"), '/ticks?stage=Morning' %>
                  </li>
                  <li class="<%= 'active-nav' if current_page?(ticks_path) && !params[:active] && !params[:stage] %>">
                    <%= link_to t("pages.header.nav.reports"), ticks_path %>
                  </li>
                  <li class="<%= 'active-nav' if current_page?(tasks_path) %>">
                    <%= link_to t("pages.header.nav.tasks"), tasks_path %>
                  </li>
                  <li>
                    <a href="/users/edit" class="btn btn-default btn-account" data-position="left">
                          <i class="glyphicon glyphicon-user"></i>
                    </a>
                  </li>
                <% else %>  
                  <li>
                    <a href="/support"><%= t "pages.header.nav.support" %></a>
                  </li>
                <% end %>
              <% else %>
                <li>
                  <a href="/features"><%= t "pages.header.nav.features" %></a>
                </li>
                <li><a href="/blog"><%= t "pages.header.nav.blog" %></a></li>
                <li>
                  <p class="navbar-btn ">
                    <a class="btn btn-default nav-btn" href="/pricing" role="button" style="color: white;"><%= t "pages.header.nav.pricing" %></a>
                  </p>
                </li>
                <li>
                  <p class="navbar-btn ">
                    <%= link_to t("pages.header.nav.login"), new_user_session_path, class: "btn btn-default nav-btn log-in-btn" %>
                  </p>
                </li>                
              <% end %>
            </ul>
          </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>

<% if user_signed_in? %>
  <div class="fixed-nav-padding">
    
  </div>
<% end %>
<script>
  $(document).on('click','.navbar-collapse.in',function(e) {
    if( $(e.target).is('a') ) {
        $(this).collapse('hide');
    }
});
</script>




