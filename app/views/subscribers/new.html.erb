	<div class="row">
		<div class="col-md-6 col-md-offset-3 panel panel-default form-style payment">
			<div class="panel-body">
				<h2>Final Step</h2>
				<h4>You have chosen the
					<% if current_user.plan == 1020 %>
						Amuse Bouche
					<% else %>
						<% if current_user.plan == 2030 %>
							Business Plan
						<% else %>
							Premium Plan
						<% end %>
					<% end %>
				</h4>
				<%= render 'form' %>
				<br>
				<%= link_to "I've changed my mind. Get me out of here!", destroy_user_session_path, method: :delete %>
			</div>
		</div>
	</div>
</div>
