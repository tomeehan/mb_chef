<%= simple_form_for(@tick) do |f| %>
	<div class="field">
		<%= select_tag(:task_id, options_for_select(@tasks), :prompt => "Select a task") %>
	</div>

	 <div class="form-material">
	 	<%= f.input :complete %>
	 	<%= f.input :temperature %>
	 	<%= f.input :staff_id %>
	 </div>

	  <div class="new-btn">
	  	<%= f.button :submit, :class => 'waves-effect btn-large' %>
	  </div>
<% end %>